<table role='presentation' cellpadding='0' cellspacing='0'>
  <tr>
    <td valign='middle' style='padding:8px 0;'>
      <a
        href='{{brandUrl}}'
        target='_blank'
        class='brand'
        style='text-decoration:none;'
      >
        {{#if logoUrl}}
          <img
            src='{{logoUrl}}'
            width='36'
            height='36'
            alt='{{brandName}}'
            style='border-radius:10px; display:block;'
          />
        {{/if}}
        <span class='brand-name'>{{brandName}}</span>
      </a>
    </td>
  </tr>
</table>